{
  "name": "baohe",
  "version": "0.0.18",
  "private": false,
  "scripts": {
    "lint": "eslint ./src --ext .js,.ts,.vue ",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "fix-all": "eslint --fix ./ --ext .vue,.js,.ts",
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cz": "git add -A && npx cz",
    "release": "node scripts/release.js",
    "postinstall": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "better-scroll": "^2.3.1",
    "element-plus": "^1.0.2-beta.48",
    "qs": "^6.10.1",
    "screenfull": "^5.1.0",
    "vue": "^3.1.1",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "autoprefixer": "^10.2.6",
    "chalk": "^4.1.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "execa": "^5.1.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "semver": "^7.3.5",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vite-plugin-imp": "^2.0.7",
    "vite-plugin-style-import": "^1.0.0"
  },
  "lint-staged": {
    "/src/*.js": [
      "prettier --write"
    ],
    "/src/*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.{js,ts,vue}": "yarn lint-fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT",
  "repository": "https://github.com/longer008/baohe.git",
  "author": "longer008"
}
